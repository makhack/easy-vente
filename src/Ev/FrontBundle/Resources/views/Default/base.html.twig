<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        
        {% stylesheets output="css/compiled/main.css" filter="cssrewrite"
            "vendor/css/bootstrap.min.css"
            "vendor/css/jcarousel.css"
            "vendor/css/flexslider.css"
            "vendor/css/style.css"
            "vendor/skins/default.css"
        %}
        <link rel="stylesheet" type="text/css" href="{{ asset_url }}" />
        {% endstylesheets %}
        
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div id="wrapper">
            <!-- start header -->
            <header>
                <div class="navbar navbar-default navbar-static-top">
                    <div class="container">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                            <a class="navbar-brand" href="index.html"><span>Easy</span> Ventes</a>
                        </div>
                        <div class="navbar-collapse collapse ">
                            <ul class="nav navbar-nav">
                                <li><a href="index.html">Home</a></li>
                                <li><a href="#">Prochaines ventes</a></li>
                                <li><a href="#">Produits star</a></li>
                                <li><a href="#">Newsletter / Alertes</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </header>
            <!-- end header -->
            {% block body %}{% endblock %}
        </div>
            {% block javascripts %}{% endblock %}
    </body>
</html>